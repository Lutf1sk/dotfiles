
# ----- background processes (void)
exec --no-startup-id swaybg -i ~/img/wallpaper
exec --no-startup-id pipewire

# output Virtual-1 mode 1920x1080

output HDMI-A-1 disable
output DP-3 mode 2560x1440@144hz adaptive_sync off pos 0 0
output DP-1 mode 1920x1080@144hz adaptive_sync off pos 2560 0

exec --no-startup-id dbus-update-activation-environment --all


# ----- program hotkeys (void)
bindsym $mod+u input type:keyboard xkb_layout us
bindsym $mod+s input type:keyboard xkb_layout se

